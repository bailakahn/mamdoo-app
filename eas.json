{
  "cli": {
    "version": ">= 3.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "base": {
      "node": "18.18.0",
      "yarn": "1.22.10"
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "ENV_NAME": "dev",
        "PROXY_URL": "http://192.168.2.68:3001",
        "PROVIDER_URL": "http://192.168.2.68:3005/",
      }
    },
    "staging": {
      "extends": "base",
      "channel": "staging",
      "env": {
        "ENV_NAME": "staging",
        "PROXY_URL": "https://staging-mamdoo-proxy.herokuapp.com",
        "PROVIDER_URL": "https://staging-mamdoo-provider.herokuapp.com/",
        "SERVICE_ACCOUNT_KEY": "credentials/staging.json"
      },
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "extends": "base",
      "channel": "production",
      "env": {
        "ENV_NAME": "production",
        "PROXY_URL": "https://prod-mamdoo-proxy.herokuapp.com",
        "PROVIDER_URL": "https://prod-mamdoo-provider.herokuapp.com/"
      },
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {
      "android": {
        "serviceAccountKeyPath": "./service-account-key-prod.json"
      }
    }
  }
}
